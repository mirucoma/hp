    "gatsby-plugin-emotion": "^7.9.0",
    "gatsby-plugin-image": "^2.9.1",
    "gatsby-plugin-output": "^0.1.3",
    "gatsby-plugin-react-helmet": "^5.9.0",
    "gatsby-plugin-sharp": "^4.9.1",
    "gatsby-plugin-typography": "^4.9.0",
    "gatsby-source-filesystem": "^4.16.0",
